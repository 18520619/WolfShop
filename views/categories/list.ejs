<div class="container mt-5">
    <a type="button" style="font-size: 20px; font-weight: 500; color: rgb(43, 28, 28);" class="btn btn-info btn-lg" href="/category/add">Add Category</a>
    <h1>List Category</h1>
<table class="table table-dark table-hover t">
    <tr>
        <th>#</th>
        <th>Name</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    
        <%
            categories.map(cate=> {%>
            <tr>
                <td>#</td>
                <td><%= cate.name%></td>
                <td><a class="btn btn-warning" href="/category/edit/<%=cate._id %>">Edit</a></td>
                <td><%- include('../part/deleteForm',{url:`/category/delete/${cate._id}`}) %></td>   
            </tr> 
            <%})
        %>
    
  </table>
</div>