<div class="container w-50 pt-5">
  <h1>Login User </h1>
  <%- include('../part/message')%>
  <form action="/user/login" method="POST">
      <div class="form-floating mb-3">
          <input type="email" class="form-control" id="name"  name="email">
          <label for="floatingInput">Email</label>
      </div>
      <div class="form-floating mb-3">
          <input type="password" class="form-control" id="dasd"  name="password">
          <label for="floatingInput">Password</label>
      </div>

      <div class="form-floating mt-3">
          <input type="submit" class="form-control  btn btn-primary" id="sub" placeholder="Save">
          
      </div>
      <div class="form-floating mt-3">
          <a href="/user/google" class="btn btn-info">Login with google</a>
          <a href="/user/github" class="btn btn-info">Login with Github</a>
          <a href="/user/facebook" class="btn btn-info">Login with Facebook</a>
      </div>
  </form>


  

</div>